{
    "cSpell.diagnosticLevel": "Hint",
    "cpplint.excludes": [
        "build",
        ".vscode"
    ],
    "cpplint.lintMode": "workspace",
    "cpplint.filters": [
        "-legal/copyright",
        "-build/include_subdir"
    ],
    "cmake.configureOnOpen": false,
    "files.associations": {
        "iomanip": "cpp",
        "iostream": "cpp",
        "array": "cpp",
        "atomic": "cpp",
        "bit": "cpp",
        "*.tcc": "cpp",
        "cctype": "cpp",
        "clocale": "cpp",
        "cmath": "cpp",
        "compare": "cpp",
        "concepts": "cpp",
        "cstdarg": "cpp",
        "cstddef": "cpp",
        "cstdint": "cpp",
        "cstdio": "cpp",
        "cstdlib": "cpp",
        "cwchar": "cpp",
        "cwctype": "cpp",
        "deque": "cpp",
        "string": "cpp",
        "unordered_map": "cpp",
        "vector": "cpp",
        "exception": "cpp",
        "algorithm": "cpp",
        "functional": "cpp",
        "iterator": "cpp",
        "memory": "cpp",
        "memory_resource": "cpp",
        "numeric": "cpp",
        "random": "cpp",
        "string_view": "cpp",
        "system_error": "cpp",
        "tuple": "cpp",
        "type_traits": "cpp",
        "utility": "cpp",
        "initializer_list": "cpp",
        "iosfwd": "cpp",
        "istream": "cpp",
        "limits": "cpp",
        "new": "cpp",
        "numbers": "cpp",
        "ostream": "cpp",
        "stdexcept": "cpp",
        "streambuf": "cpp",
        "typeinfo": "cpp",
        "cinttypes": "cpp",
        "ctime": "cpp",
        "sstream": "cpp",
        "bitset": "cpp",
        "charconv": "cpp",
        "chrono": "cpp",
        "codecvt": "cpp",
        "condition_variable": "cpp",
        "cstring": "cpp",
        "optional": "cpp",
        "ratio": "cpp",
        "fstream": "cpp",
        "mutex": "cpp",
        "semaphore": "cpp",
        "span": "cpp",
        "stop_token": "cpp",
        "thread": "cpp",
        "variant": "cpp",
        "format": "cpp",
        "__nullptr": "cpp",
        "filesystem": "cpp",
        "__hash_table": "cpp",
        "__split_buffer": "cpp",
        "queue": "cpp",
        "stack": "cpp",
        "map": "cpp",
        "regex": "cpp",
        "complex": "cpp",
        "forward_list": "cpp",
        "list": "cpp",
        "set": "cpp",
        "unordered_set": "cpp",
        "ranges": "cpp",
        "typeindex": "cpp",
        "valarray": "cpp"
    },
    "C_Cpp.errorSquiggles": "disabled",
    "C_Cpp.default.compilerPath": "/usr/bin/gcc",

    "python.envFile": "${workspaceFolder}/.env",

    "python.analysis.extraPaths": [
    "./build"
],
"python.analysis.stubPath": "./build",
"python.analysis.autoSearchPaths": true,
"python.analysis.diagnosticSeverityOverrides": {
    "reportMissingImports": "none",
    "reportMissingModuleSource": "none"
},
"python.testing.pytestArgs": [
    "GUI"
],
"python.testing.unittestEnabled": false,
"python.testing.pytestEnabled": true
}
